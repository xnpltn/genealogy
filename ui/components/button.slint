import { Button } from "std-widgets.slint";
export component CustomButton inherits Rectangle {
    in property <string> title;
    in property <brush> background-color;
    callback clicked();
    border-radius: 5px;
    background: background-color;
    t := Text {
        text: title;
        color: #fff;
    }

    TouchArea {
        width: 100%;
        height: 100%;
        clicked => {
            parent.clicked();
        }
    }
}
